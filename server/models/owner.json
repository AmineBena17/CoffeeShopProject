{
  "name": "owner",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "nom": {
      "type": "string",
      "required": true
    },
    "prenom": {
      "type": "string",
      "required": true
    },
    "role": {
      "type": "string",
      "required": true
    },
    "datenaissance": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "coffeeshops": {
      "type": "hasMany",
      "model": "coffeeshop",
      "foreignKey": "idsCoffeshops"
    },
    "restaurants": {
      "type": "hasMany",
      "model": "restaurant",
      "foreignKey": "idsRestaurant"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "DENY"
    }
  ],
  "methods": {}
}
